{
    "compilerOptions": {
        "target": "ES6",
        "sourceMap": true,
        "lib": [
            "ES6",
            "dom",
            "dom.iterable"
        ],
        "module": "ES6",
        "outDir": "compiled",
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "alwaysStrict": true,
        "exactOptionalPropertyTypes": true,
        "noImplicitAny": true,
        "noImplicitOverride": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noPropertyAccessFromIndexSignature": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strict": true
    }
}